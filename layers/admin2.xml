<Map srs="+init=epsg:3857">
  <Style name="polygons">
    <Rule>
      <PolygonSymbolizer fill="#374C70" />
      <LineSymbolizer stroke="#00FFFF" stroke-width="0.25" stroke-opacity="0.25" />
    </Rule>
  </Style>
  <Layer name="world" srs="+init=epsg:4326">
    <StyleName>polygons</StyleName>
    <Datasource>
      <Parameter name="file">./tl_2010_us_county10.shp</Parameter>
      <Parameter name="type">shape</Parameter>
    </Datasource>
  </Layer>
</Map>